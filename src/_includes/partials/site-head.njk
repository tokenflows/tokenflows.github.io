<meta charset="UTF-8">
<meta http-equiv="x-ua-compatible" content="ie=edge">
<meta name="viewport" content="width=device-width, minimum-scale=1.0">
<title>{{ title or site.title }}</title>
{% if description %}
	<meta name="description" content="{{ description }}">
{% endif %}
{% if keywords %}
	<meta name="keywords" content="{{ keywords }}">
{% endif %}
<link rel="apple-touch-icon" sizes="180x180" href="{{ site.baseurl}}/assets/images/favico/apple-touch-icon.png">
<link rel="icon" type="image/x-icon" href="{{ site.baseurl}}/assets/images/favico/favicon.ico">
<link rel="icon" type="image/png" sizes="32x32" href="{{ site.baseurl}}/assets/images/favico/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="16x16" href="{{ site.baseurl}}/assets/images/favico/favicon-16x16.png">
<link rel="manifest" href="{{ site.baseurl}}/assets/images/favico/site.webmanifest">
<link rel="mask-icon" href="{{ site.baseurl}}/assets/images/favico/safari-pinned-tab.svg" color="#5bbad5">
<meta name="msapplication-TileColor" content="#da532c">
<meta name="theme-color" content="#ffffff">

<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Montserrat:400,400i,500,500i,700,700i|Noto+Sans:400,400i,700,700i|Source+Code+Pro&amp;subset=latin-ext">
<link rel="stylesheet" href="{{ site.baseurl}}/assets/css/style.css">
