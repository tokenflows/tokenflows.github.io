<meta charset="UTF-8">
<meta http-equiv="x-ua-compatible" content="ie=edge">
<meta name="viewport" content="width=device-width, minimum-scale=1.0">
<title>{{ title or site.title }}</title>
{% if description %}
	<meta name="description" content="{{ description }}">
{% endif %}
{% if keywords %}
	<meta name="keywords" content="{{ keywords }}">
{% endif %}
<link rel="icon" href="{% if environment == 'production' %}{{ site.baseurl}}{% endif %}/favicon.ico" type="image/x-icon">
<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Montserrat:400,400i,500,500i,700,700i|Noto+Sans:400,400i,700,700i|Source+Code+Pro&amp;subset=latin-ext">
<link rel="stylesheet" href="{% if environment == 'production' %}{{ site.baseurl}}{% endif %}/assets/css/style.css">
